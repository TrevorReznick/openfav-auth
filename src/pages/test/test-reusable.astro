---
import Layout from '../../layouts/Layout.astro'
import IsComponent from '../../components/test/MyComponent.astro'

import { email, user_name } from '../../store'

const _email = email.get()
const _user_name = user_name.get()

---

<Layout title="Login">
  <IsComponent
    email={_email}
    user_name={_user_name}
    tagline="Welcome"
    title="Discover the future of Astro"
    subtitle="Flexible, performant, and modular"
    content="Discover a new level of data analysis with our innovative and user-friendly platform. Transform your business needs with actionable insights."
    
  />
</Layout>
